      },
      "package": "com.sythai0611.ticket_app",
      "googleServicesFile": "./google-services.json",
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "ec2c34b0-cd5c-4874-afd4-8595eea31827"
